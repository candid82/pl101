start =
    expression

validchar
    = [0-9a-zA-Z_?!+\-=@#$%^&*/.]

atom =
    chars:validchar+
        { return chars.join(""); }

quote =
    "'" expr:expression {return ['quote', expr];}


expression =
  sep* atom:atom sep* {return atom;}
/ sep* quote:quote sep* {return quote;}
/ sep* '(' first:expression rest:expression* ')' sep*
    { return [first].concat(rest); }

sep =
  [ \t\r\n]
